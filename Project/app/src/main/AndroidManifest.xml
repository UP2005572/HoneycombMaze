<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.honeycombmaze">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HoneycombMaze">
        <activity
            android:name=".SuccessfulRun"
            android:exported="false"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.HoneycombMaze.noActionBar"/>
        <activity
            android:name=".FallenOutScreen"
            android:exported="false"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.HoneycombMaze.noActionBar" />
        <activity
            android:name=".IntroScreen"
            android:exported="false"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.HoneycombMaze.noActionBar" />
        <activity
            android:name=".InChamberActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.HoneycombMaze.noActionBar" />
        <activity
            android:name=".StartScreen"
            android:exported="true"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.HoneycombMaze.noActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>